	<script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
	<![endif]-->

</head>
<body class="hold-transition skin-blue sidebar-mini">

<div class="wrapper">

	<header class="main-header">
		<nav class="navbar navbar-static-top">
			<a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
				<span class="sr-only">Toggle navigation</span>
			</a>
			<div class="navbar-custom-menu">
				<ul class="nav navbar-nav">
					<li class="search-box">
						<a class="nav-link hidden-sm-down" href="javascript:void(0)"><i class="mdi mdi-magnify"></i></a>
						<form class="app-search" style="display: none;">
							<input type="text" class="form-control" placeholder="Search &amp; enter"> <a class="srh-btn"><i class="ti-close"></i></a>
						</form>
					</li>
					<li class="dropdown user user-menu">
						<a href="{{ path('fos_user_security_logout') }}" class="dropdown-toggle">
							Deconnexion
						</a>
					</li>
					<li>
						<a href="#" data-toggle="control-sidebar"><i class="fa fa-cog fa-spin"></i></a>
					</li>
				</ul>
			</div>
		</nav>
	</header>
	<aside class="main-sidebar">
		<section class="sidebar">
			<div class="user-panel">
				<div class="image">
					<img src="{{ asset('backoffice/image/jamboree.png') }}" class="rounded-circle" alt="Logo jamboree">
				</div>
			</div>
			<!-- sidebar menu -->
			<ul class="sidebar-menu" data-widget="tree">
				<li class="nav-devider"></li>
				<li class="header nav-small-cap">Rubrique</li>
				<li class="active">
					<a href="{{ absolute_url(path('homepage')) }}">
						<i class="icon-home"></i> <span>Tableau de bord</span>
						<span class="pull-right-container">
                                      <i class="fa fa-angle-right pull-right"></i>
                                    </span>
					</a>
				</li>
				<li>
					<a href="{{ path('region_liste') }}">
						<i class="icon-home"></i> <span>Régions</span>
						<span class="pull-right-container">
                                      <i class="fa fa-angle-right pull-right"></i>
                                    </span>
					</a>
				</li>
				<li>
					<a href="{{ path('participant_index') }}">
						<i class="icon-home"></i> <span>Participants</span>
						<span class="pull-right-container">
                                      <i class="fa fa-angle-right pull-right"></i>
                                    </span>
					</a>
				</li>
				<!--<li class="treeview">
                                <a href="#">
                                    <i class="icon-chart"></i>
                                    <span>Branches</span>
                                    <span class="pull-right-container">
                                      <i class="fa fa-angle-right pull-right"></i>
                                    </span>
                                </a>
                                <ul class="treeview-menu">
                                </ul>
                            </li>-->
				<!--<li class="treeview">
					<a href="#">
						<i class="icon-compass"></i>
						<span>Regions</span>
						<span class="pull-right-container">
                                      <i class="fa fa-angle-right pull-right"></i>
                                    </span>
					</a>
					<ul class="treeview-menu">
						{{ render(url('region')) }}
					</ul>
				</li>-->
				<li class="treeview">
					<a href="#">
						<i class="icon-compass"></i>
						<span>Branches</span>
						<span class="pull-right-container">
                                      <i class="fa fa-angle-right pull-right"></i>
                                    </span>
					</a>
					<ul class="treeview-menu">
						{{ render(url('branche')) }}
					</ul>
				</li>
			</ul>
		</section>
	</aside>

	{% block body %}
	{% endblock %}

	<footer class="main-footer">
		&copy; 2019 <a href="https://www.scoutascci.org/">Association des Scouts Catholiques de Côte d'Ivoire</a>. &nbsp; | &nbsp; Tous droits réservés.
	</footer>
	<div class="control-sidebar-bg"></div>

</div>

