	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
	<!--<meta http-equiv="refresh" content="60">-->
	<title>{% block title %}Jamboree 2019{% endblock %}</title>
	<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
	{% block stylesheets %}
		<link rel="stylesheet" href="{{ absolute_url(asset('backoffice/assets/vendor_components/bootstrap/dist/css/bootstrap.css')) }}">
		<link rel="stylesheet" href="{{ absolute_url(asset('backoffice/assets/vendor_plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.css')) }}">
		<link rel="stylesheet" href="{{ absolute_url(asset('backoffice/css/bootstrap-extend.css')) }}">
		<link rel="stylesheet" href="{{ absolute_url(asset('backoffice/css/master_style.css')) }}">
		<link rel="stylesheet" href="{{ absolute_url(asset('backoffice/css/skins/_all-skins.css')) }}">
	{% endblock %}
